{
  "expo": {
    "name": "MyPOCApp",
    "slug": "MyPOCApp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mypocapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    // "webEngage": {
    //   "file": "./plugins/WebEngageExpo.java",
    //   "packageName": "com.webengage.reactSample"
    // },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webengage.reactSample"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.webengage.reactSample"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "./plugins/expo-webengage-core",
        {
          "environment": "us",
          "file": "./plugins/WebEngageExpo.java",
          "packageName": "com.webengage.reactSample"
        }
      ],
      [
        "./plugins/expo-webengage-native-push",
        {
          "messagingServiceFile": "./plugins/MyFirebaseMessagingService.java"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "51011d93-9048-43b3-b646-84b05ca02a94"
      }
    }
  }
}
